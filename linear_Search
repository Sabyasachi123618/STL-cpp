#include<stdio.h>
int main()
{
	int a[20],n,i;
	int flag=1;
	printf("Enter the number of elements:");
	scanf("%d",&n);
	for(i=0;i<n;i++)
	{
		printf("Enter the elemene[%d]:",i);
		scanf("%d",&a[i]);
	}
	int key;
	printf("Enter the element to be searched:");
	scanf("%d",&key);
	for(i=0;i<n;i++)
	{
		if(key==a[i])
		{
			printf("Element %d found at pos %d",key,i);
			flag=0;
			break;
		}	
	}
	if(flag==1)
	{
		printf("Element not found\n");
	}
	return 0;
}
